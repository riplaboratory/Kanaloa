<launch>
  <node name="usb_cam5" pkg="usb_cam" type="usb_cam_node" output="screen" >
    <param name="video_device" value="/dev/cam5" />
    <param name="image_width" value="640" />
    <param name="image_height" value="360" />
    <param name="pixel_format" value="yuyv" />
    <param name="camera_frame_id" value="usb_cam" />
    <param name="io_method" value="mmap"/>
<param name="autoexposure" value="false"/>
  </node>
<!--  <node name="image_view" pkg="image_view" type="image_view" respawn="false$
    <remap from="image" to="/usb_cam5/image_raw"/>
    <param name="autosize" value="true" />
  </node>
-->
</launch>



